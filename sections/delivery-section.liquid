<div class="grid">
  <div class="grid__item large--four-fifths push--large--one-tenth">
    <div class="rte rte--nomargin rte--indented-images">
		{% for blocks in section.blocks %}
      		<p class="p1 title">
              	<strong>{{ block.sections.title }}</strong>
      		</p>
      		<p>
            	{{ block.sections.content }}
      		</p>
    	{% endfor %}
    </div>
  </div>
</div>

{% schema %}
  {
    "name": "FAQ",
	"blocks": [
		{
			"type": "delivery",
			"name": "Delivery",
			"settings": [
				{
					"id": "title",
					"type": "text",
					"label": "Title",
					"default": "Refund:"
				},
				{
					"id": "content",
					"type": "richtext",
					"label": "Content"
				}
			]
		}
	]
  }
{% endschema %}
